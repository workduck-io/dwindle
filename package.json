{
  "name": "@workduck-io/dwindle",
  "version": "0.1.7",
  "description": "AWS requests for HTTP",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workduck-io/dwindle.git"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/workduck-io",
    "files": [
      "lib/**/*"
    ]
  },
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "homepage": "https://github.com/workduck-io/dwindle#readme",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "eslint": "^8.25.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "nodemon": "^2.0.20",
    "ky": "^0.32.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.4.4",
    "zustand": "^3.5.13"
  },
  "peerDependencies": {
    "ky": "^0.32.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^3.5.13"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.17.0",
    "@aws-sdk/client-s3": "^3.17.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.17.0",
    "@aws-sdk/s3-request-presigner": "^3.17.0",
    "@aws-sdk/util-endpoints": "^3.192.0",
    "@types/jsonwebtoken": "^8.5.5",
    "amazon-cognito-identity-js": "^5.2.0",
    "buffer": "^6.0.3",
    "cross-fetch": "^3.1.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "nanoid": "^4.0.0",
    "process": "^0.11.10",
    "qs": "^6.10.3"
  }
}
